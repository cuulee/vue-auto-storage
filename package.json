{
  "name": "vue-auto-storage",
  "version": "1.0.0",
  "description": "An automatic storage plugin for Vue2, persist state in `data` with localStorage.",
  "main": "dist/vue-auto-storage.js",
  "module": "src/index.js",
  "typings": "types/index.d.ts",
  "scripts": {
    "dev": "rimraf dist && cross-env NODE_ENV=development rollup -c scripts/rollup.config.prod.js",
    "build": "rimraf dist && cross-env NODE_ENV=production rollup -c scripts/rollup.config.prod.js",
    "dev:demo": "vue-cli-service serve",
    "build:demo": "rimraf docs && vue-cli-service build",
    "test": "cross-env NODE_ENV=test && jest",
    "release": "standard-version"
  },
  "files": [
    "README.md",
    "src",
    "dist/*.js",
    "types/*.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zh-rocco/vue-auto-storage.git"
  },
  "keywords": [
    "persistent-storage",
    "vue-auto-storage",
    "auto-storage",
    "vue-plugin",
    "vue"
  ],
  "author": {
    "name": "Rocco Mormont",
    "email": "rocco.mormont@gmail.com",
    "url": "https://zh-rocco.github.io/"
  },
  "bugs": {
    "url": "https://github.com/zh-rocco/vue-auto-storage/issues"
  },
  "engines": {
    "node": ">=6"
  },
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.56",
    "@babel/plugin-external-helpers": "^7.0.0-beta.56",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.56",
    "@babel/preset-env": "^7.0.0-beta.56",
    "@babel/runtime": "^7.0.0-beta.56",
    "@vue/cli-service": "^3.0.0-rc.11",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "cross-env": "^5.2.0",
    "dom-storage": "^2.1.0",
    "jest": "^23.4.2",
    "lodash.capitalize": "^4.2.1",
    "regenerator-runtime": "^0.12.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.63.4",
    "rollup-plugin-babel": "^4.0.0-beta.7",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-filesize": "^4.0.1",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^4.0.0",
    "standard-version": "^4.4.0",
    "uglify-es": "^3.3.9",
    "vue": "2.5.16"
  }
}
